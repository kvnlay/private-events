<%= form_for(@event, url: events_path) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :date %>
  <%= f.date_field :date %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit "Create my event" %>
<% end %>
