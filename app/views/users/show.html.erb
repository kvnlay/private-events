<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
  <div>
    <%= @events.each do |event|%>
      <%= event.name%>
      <%= event.location %>
      <%= event.date %>
    <%end%>
  </div>
</div>

<%= @user.event_as_attendee.each do |event| %>
  <li><%= event %></li>
<% end %>